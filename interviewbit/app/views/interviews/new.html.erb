<div class="justify-content-center ml-auto mr-auto container">
<h1>New Interview Creation Page</h1>
<h4>
	<%= form_with scope: :interview , url: interviews_path, local: true do |f| %>
		<div>
			<%= f.label :title %>
			<%= f.text_field :title, class: 'form-control' %>
		</div>
		<div>
			<%= f.label :start %>
			<%= f.date_field :start,  class: 'form-control' %>
		</div>
		<div>
			<%= f.label :start_time %>
			<%= f.time_field :start_time,  class: 'form-control' %>
		</div>
		<div>
			<%= f.label :end %>
			<%= f.date_field :end,  class: 'form-control' %>
		</div>
		<div>
			<%= f.label :end_time %>
			<%= f.time_field :end_time,  class: 'form-control' %>
		</div>
		<%= f.submit %>
	<%end%>
</h4>
</div>