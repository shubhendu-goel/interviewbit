<div class="justify-content-center ml-auto mr-auto container">
<h3><%= link_to 'All Users', users_path , class: 'btn btn-success'%></h3>
<h1>New User Creation Page</h1>
<h4>
	<%= form_with scope: :user , url: users_path, local: true do |f| %>
		<div>
			<%= f.label :name %>
			<%= f.text_field :name, class: 'form-control' %>
		</div>
		<div>
			<%= f.label :email %>
			<%= f.email_field :email,  class: 'form-control' %>
		</div>
		<div>
			<%= f.label :resume %><br>
		 	<%= f.file_field  :resume ,class: 'form-control' %>
		</div>
		<%= f.submit %>
	<%end%>
</h4>
</div>